input {
    stdin {
        type = "stdin-type"
    }
    redis {
        type = "redis-queue"
        host = "localhost"
        type = "list"
        key  = "seshat"
    }
}

filter {
    regex {
      pattern = "^([0-9]*).*$"
      fields = [ "numeric_id" ]
    }
    alter {
      remove_field = [ "coco" ]
      add_fields = [
        { number = true },
        { filtered = true }
      ]
    }
}

output {
    stdout {
      debug = true
      debug_format = "json"
    }
}
